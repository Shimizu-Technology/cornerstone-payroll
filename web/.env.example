# Cornerstone Payroll Frontend - Environment Variables

# API URL
VITE_API_URL=http://localhost:3000/api/v1

# Development company scope (used when auth is disabled)
VITE_COMPANY_ID=1

# WorkOS AuthKit
VITE_WORKOS_CLIENT_ID=client_xxx
VITE_WORKOS_REDIRECT_URI=http://localhost:5173/callback

# Authentication (set to 'true' to enable WorkOS auth)
VITE_AUTH_ENABLED=false
